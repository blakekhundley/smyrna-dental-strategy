        <!-- Website Structure -->
        <section id="website-structure" class="mb-12">
          <h2 class="text-2xl font-bold dental-blue mb-6 pb-2 border-b-2 border-gray-200">4. Comprehensive Website Structure</h2>
          
          <div class="dental-card bg-white rounded-lg p-6 mb-6">
